<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prajwala.quizapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Instructions"
            android:label="@string/title_activity_instructions" >
        </activity>
        <activity
            android:name=".QuestionPage1"
            android:label="@string/title_activity_question_page1" >
        </activity>
        <activity
            android:name=".QuestionPage2"
            android:label="@string/title_activity_question_page2" >
        </activity>
        <activity
            android:name=".QuestionPage3"
            android:label="@string/title_activity_question_page3" >
        </activity>
        <activity
            android:name=".QuestionPage4"
            android:label="@string/title_activity_question_page4" >
        </activity>
        <activity
            android:name=".QuestionPage5"
            android:label="@string/title_activity_question_page5" >
        </activity>
        <activity
            android:name=".Score"
            android:label="@string/title_activity_score" >
        </activity>
    </application>

</manifest>
