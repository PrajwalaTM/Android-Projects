<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.prajwala.quizapp.QuestionPage5"
    android:background="#ffedffbc">

    <TextView android:text="@string/questionfive" android:layout_width="350dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="72dp"
        android:singleLine="false"
        android:layout_alignParentEnd="false"
        android:textIsSelectable="false"
        android:id="@+id/textView9"
        android:textColor="#fb040404"
        android:textSize="20sp" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="SPSS"
        android:id="@+id/q5o1"
        android:layout_below="@+id/textView9"
        android:layout_centerHorizontal="true"
        android:onClick="wrong"
        android:layout_alignParentStart="false"
        android:visibility="visible"
        android:background="@mipmap/scorepic"
        android:textColor="#fafffdfb" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="GPSS"
        android:id="@+id/q5o2"
        android:layout_below="@+id/q5o1"
        android:layout_centerHorizontal="true"
        android:onClick="correct"
        android:visibility="visible"
        android:textColor="#ffffffff"
        android:background="@mipmap/scorepic" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="COBOL"
        android:id="@+id/q5o3"
        android:layout_below="@+id/q5o2"
        android:layout_centerHorizontal="true"
        android:onClick="wrong"
        android:visibility="visible"
        android:background="@mipmap/scorepic"
        android:textColor="#ffffffff" />
    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="DL"
        android:id="@+id/q5o4"
        android:layout_below="@+id/q5o3"
        android:layout_centerHorizontal="true"
        android:onClick="wrong"
        android:visibility="visible"
        android:background="@mipmap/scorepic"
        android:textColor="#ffffffff"
        android:singleLine="false" />

    <TextView
        android:layout_width="300dp"
        android:layout_height="25dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/score5"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="PROCEED"
        android:id="@+id/proceed5"
        android:background="#e3184fff"
        android:textColor="#ffffffff"
        android:onClick="movenext"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
